<div class="_small-modal-container">
  <div class="_modal-background _column-positioner _hcenter-positioner">
    <div class="_small-modal-title text-center">
      <%= image_tag "icon_qna", class: "_modal-big-icon-m" %>
      <p class="_fs-4-mobile _white _f-letter-mobile">정말 로그아웃 하시겠어요?</p>
    </div>
    <div class="_row-positioner">
      <%= link_to(destroy_user_session_path, method: "DELETE", class: "_small-modal-button _vcenter-positioner _hcenter-positioner") do %>
          <p class="_fs-3-mobile _red">로그아웃</p>
      <% end %>
      <div class="_small-modal-button _vcenter-positioner _hcenter-positioner" data-dismiss="modal">
        <p class="_fs-3-mobile _white">취소</p>
      </div>
    </div>
  </div>
</div>