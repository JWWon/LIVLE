var emptyLikeIcon = $('#icon-<%= @like_type %>-empty');
var filledLikeIcon = $('#icon-<%= @like_type %>-filled');

<% if @like_true %>
emptyLikeIcon.addClass("_display-none");
filledLikeIcon.removeClass("_display-none");
<% else %>
emptyLikeIcon.removeClass("_display-none");
filledLikeIcon.addClass("_display-none");
<% end %>

$('#<%= @like_type %>-like-count').text("<%= @like_count %>");

// only for upcoming show page video like data
<% if @video_index %>
console.log($("#video-like-count").text());
$('#video-like-count').text("<%= @like_count %>");
$('.show-video-container').filter(function(index, container) {
    return $(container).data("index") == <%= @video_index %>;
}).data("likeTrue", <% if @like_true %>true<% else %>false<% end %>);
<% end %>