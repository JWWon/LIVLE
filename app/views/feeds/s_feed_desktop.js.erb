-$('#feed-list-container').append('<%= j render partial: "index_card_desktop", collection: @feeds, as: :feed %>');
$('#feed-list-container').append('<%= j render partial: "curations/curation_card_desktop", collection: @curations, as: :curation %>');
<% if @feeds.next_page || @curations.next_page %>
  $('.pagination').replaceWith('<%= j will_paginate @feeds %>');
  $('.pagination').replaceWith('<%= j will_paginate @curations %>');
<% else %>
  $(window).off('scroll');
  $('.pagination').remove();
<% end %>
