<div id="feeds-index-mobile">
  <div class="_hcenter-positioner">
    <%= image_tag "logo_livle", class: "_logo-container-m" %>
  </div>

  <div class="_list-container-m" id="feeds-card-list-m">
    <% @feeds.each do |feed| %>
        <%= render partial: 'index_card_mobile', locals: {feed: feed} %>
    <% end %>
  </div>

  <div id="infinite-scrolling">
    <%= will_paginate @feeds %>
  </div>
</div>
