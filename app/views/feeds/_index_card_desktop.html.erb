<div id="feed-index-card">
  <% video_id = feed.youtube_id.gsub(/https:\/\/www.youtube.com\/watch\?v=|https:\/\/youtu.be\//, '') %>
  <%= link_to feed_path(feed.id) do %>
      <div class="_vcenter-positioner _no-p-margin _text-container">
        <%= image_tag("icon_crown", class: "_thumbnail-info-icon _crown-icon-margin", style: "margin-top: -1%") %>
        <p class="_fs-5 _white"><%= feed.user.nickname %></p>
      </div>

      <div class="_thumbnail-image-container">
        <%= image_tag "image_live", class: "_thumbnail-image", id: "feed-#{feed.id}" %>
      </div>

      <div class="_text-container _no-p-margin">
        <p class="_fs-5 _light-gray">
          <%= feed.title %> -
          <!-- artist name -->
          <% feed.artists.each_with_index do |artist, i| %>
              <%= artist.name %>
              <% break if feed.artists[i+1] == nil %>
              ,&nbsp
          <% end %>
        </p>
        <p class="_fs-5 _light-gray">조회수 <%= feed.count_view %></p>
      </div>
  <% end %>
</div>
<script>
    $('#feed-<%= feed.id %>').attr({
        src: "http://img.youtube.com/vi/<%= video_id %>/maxresdefault.jpg"
    });
</script>