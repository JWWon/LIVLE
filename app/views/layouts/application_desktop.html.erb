  <!-- Global Site Tag (gtag.js) - Google Analytics -->
  <script async src="https://www.googletagmanager.com/gtag/js?id=UA-104199726-1"></script>
  <script>
      function redir(category, action, label, value, where) {
          ga('send', 'event', category, action, label, value);
          (window.location=where);
      }

      window.dataLayer = window.dataLayer || [];
      function gtag(){dataLayer.push(arguments)};
      gtag('js', new Date());

      gtag('config', 'UA-104199726-1');
  </script>
</head>

<body>
  <!-- Facebook Inclusion -->
  <script>
  window.fbAsyncInit = function() {
    FB.init({
      appId            : '<%=ENV["Facebook_Key"]%>',
      autoLogAppEvents : true,
      xfbml            : true,
      version          : 'v2.10'
    });
    FB.AppEvents.logPageView();
  };

  (function(d, s, id){
    var js, fjs = d.getElementsByTagName(s)[0];
    if (d.getElementById(id)) {return;}
    js = d.createElement(s); js.id = id;
    js.src = "//connect.facebook.net/en_US/sdk.js";
    fjs.parentNode.insertBefore(js, fjs);
  }(document, 'script', 'facebook-jssdk'));
  </script>
    <!-- Render 우선순위로 나열 -->
<%= render partial: "partial_views/loading_modal" %>

<%= render partial: "partial_views/navbar_desktop"%>
<% unless @enable_transparent %>
    <div style="padding: 2.5rem; background: #121212;"></div>
<% end %>
<%= yield %>
<% if @enable_footer %>
    <%= render partial: "partial_views/footer_desktop" %>
<% end %>
</body>

</html>
